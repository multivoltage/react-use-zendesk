{
  "name": "react-use-zendesk",
  "author": {
    "name": "Diego Tonini",
    "email": "diego.tonini93@gmail.com"
  },
  "description": "React Zendesk Web Widget integration written in typescript",
  "license": "MIT",
  "version": "1.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/multivoltage/react-use-zendesk"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "browser": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "zendesk",
    "zendesk-chat",
    "react-zendesk",
    "zendesk-react",
    "zendesk web widget",
    "react-use-zendesk",
    "zendesk-next",
    "next-zendesk",
    "zendesk-widget"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "tsup",
    "dev": "tsup src/index.ts --format cjs,esm --sourcemap --watch --dts --external react"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.5",
    "@types/eslint": "^8.56.1",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "react": "^18.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  }
}
